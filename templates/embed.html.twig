<iframe
    allowfullscreen
    mozallowfullscreen="true"
    webkitallowfullscreen="true"
    width="{{ width }}"
    height="{{ height }}"
    frameborder="0"
    style="border:0;"
    onmousewheel=""
    src="https://api.cappasity.com/api/player/{{ viewId }}/embedded?autorun={{ autoRun ? 1 : 0 }}&closebutton={{ closeButton ? 1 : 0 }}&logo={{ logo ? 1 : 0 }}&autorotate={{ autoRotate ? 1 : 0 }}&autorotatetime={{ autoRotateTime }}&autorotatedelay={{ autoRotateDelay }}&autorotatedir={{ autoRotateDir }}&hidefullscreen={{ hideFullScreen ? 1 : 0 }}&hideautorotateopt={{ hideAutoRotateOpt ? 1 : 0 }}&hidesettingsbtn={{ hideSettingsBtn ? 1 : 0 }}&enableimagezoom={{ enableImageZoom ? 1 : 0 }}&zoomquality={{ zoomQuality }}&hidezoomopt={{ hideZoomOpt ? 1 : 0 }}&analytics={{ analytics ? 1 : 0 }}"
></iframe>
